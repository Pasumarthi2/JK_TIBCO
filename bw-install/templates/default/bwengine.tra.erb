#==========================================================
# *** Common variables. Modify these only. ***
#==========================================================
#
tibco.env.PSP=:
tibco.env.TIB_HOME=<%= node['bw-install']['install']['tibco_install_dir'] %>
tibco.env.JVM_LIB_PATH=<%= node['bw-install']['install']['tibco_install_dir'] %>/tibcojre/<%= node['bw-install']['install']['tibcojre_version'] %>/lib/i386/server/libjvm.so
tibco.env.JVM_LIB_DIR=<%= node['bw-install']['install']['tibco_install_dir'] %>/tibcojre/<%= node['bw-install']['install']['tibcojre_version'] %>/lib/i386
tibco.env.JVM_LIB_SERVER_DIR=<%= node['bw-install']['install']['tibco_install_dir'] %>/tibcojre/<%= node['bw-install']['install']['tibcojre_version'] %>/lib/i386/server
tibco.env.RV_HOME=<%= node['bw-install']['install']['tibco_install_dir'] %>/tibrv/<%= node['bw-install']['install']['tibrv_version'] %>
tibco.env.TIB_JAVA_HOME=<%= node['bw-install']['install']['tibco_install_dir'] %>/tibcojre/<%= node['bw-install']['install']['tibcojre_version'] %>
tibco.env.TPCL_HOME=<%= node['bw-install']['install']['tibco_install_dir'] %>/tpcl/<%= node['bw-install']['install']['tpcl_version'] %>
tibco.env.TRA_HOME=<%= node['bw-install']['install']['tibco_install_dir'] %>/tra/<%= node['bw-install']['install']['tra_version'] %>
tibco.env.TRA_APPEND_VERSION=<%= node['bw-install']['install']['tra_version'] %>
tibco.env.HAWK_HOME=<%= node['bw-install']['install']['tibco_install_dir'] %>/hawk/<%= node['bw-install']['install']['hawk_version'] %>
tibco.env.BW_HOME=<%= node['bw-install']['install']['tibco_install_dir'] %>/bw/<%= node['bw-install']['install']['bw_version'] %>
tibco.env.BW_PLUGINS_HOME=<%= node['bw-install']['install']['tibco_install_dir'] %>/bw/plugins

#
# Other arguments to application, JVM etc.
#
tibco.env.APP_ARGS=
tibco.env.HEAP_SIZE=256M

java.property.TIBCO_SECURITY_VENDOR j2se
#
# Customizable Classpath information...
#    e.g g:/a1/b1;d:/a2/b2/c.jar
#    All classes and jars in these directories will be automatically picked
#    up. You can also specify individual files. Use forward slashes only.
#
tibco.env.CUSTOM_EXT_PREPEND_CP=%BW_HOME%/hotfix/lib:%TRA_HOME%/hotfix/lib:%RV_HOME%/lib/tibrvj.jar:%RV_HOME%/lib:%TPCL_HOME%/lib/mail.jar:%TRA_HOME%/lib/TIBC
rypt.jar
tibco.env.CUSTOM_EXT_APPEND_CP=

#
#==========================================================
# *** Do NOT modify beyond this line ***
#==========================================================
#
# TIBCO standard classpath
#
tibco.env.STD_EXT_CP %TIB_JAVA_HOME%/lib:%BW_HOME%/hotfix/lib:%BW_HOME%/lib:%BW_HOME%/lib/palettes:%BW_PLUGINS_HOME%/lib:%BW_PLUGINS_HOME%/lib/palettes:%TRA_HOME%/hotfix/lib:%TRA_HOME%/lib:%TRA_HOME%/hotfix/lib/palettes:%TRA_HOME%/lib/palettes:%TRA_HOME%/lib/repository_mappers:%TRA_HOME%/hotfix/icjava/6.2/lib:%TRA_HOME%/icjava/6.2/lib:%TPCL_HOME%/lib/httpclient_3.0:%TPCL_HOME%/jdbc:%TPCL_HOME%/tomcat/7/bin:%TPCL_HOME%/tomcat/7/lib:%TPCL_HOME%/lib:%HAWK_HOME%/lib:%HAWK_HOME%/lib/compat:%TPCL_HOME%/lib/entrust

#
# TIBCO Standard Path (Applicable to all Platforms)
#
tibco.env.PATH %BW_HOME%/hotfix/bin:%BW_HOME%/bin:%TRA_HOME%/hotfix/bin:%TRA_HOME%/bin:%TRA_HOME%/hotfix/icjava/6.2/lib:%TRA_HOME%/icjava/6.2/lib:%TRA_HOME%/hotfix/icjava/6.2/bin:%TRA_HOME%/icjava/6.2/bin:%TPCL_HOME%/bin:%RV_HOME%/bin:%HAWK_HOME%/bin:<%= node['bw-install']['install']['tibco_install_dir'] %>/tibcojre/<%= node['bw-install']['install']['tibcojre_version'] %>/bin:%PATH%

#
# TIBCO Load Library Path
# Load Library Path (LD_LIBRARY_PATH, SHLIB_PATH, LIBPATH) are ignored
# on Windows environments.
#
tibco.env.LD_LIBRARY_PATH %TPCL_HOME%/lib/httpclient_3.0:%TRA_HOME%/hotfix/lib:%TRA_HOME%/lib:%TPCL_HOME%/lib:%RV_HOME%/lib:%JVM_LIB_SERVER_DIR%:%JVM_LIB_DIR%:%TRA_HOME%/hotfix/icjava/6.2/lib:%TRA_HOME%/icjava/6.2/lib:%LD_LIBRARY_PATH%

tibco.env.SHLIB_PATH %TPCL_HOME%/lib/httpclient_3.0:%TRA_HOME%/hotfix/lib:%TRA_HOME%/lib:%TPCL_HOME%/lib:%RV_HOME%/lib:%JVM_LIB_SERVER_DIR%:%JVM_LIB_DIR%:%TRA_HOME%/hotfix/icjava/6.2/lib:%TRA_HOME%/icjava/6.2/lib:%SHLIB_PATH%

tibco.env.LIBPATH %RV_HOME%/lib:%TRA_HOME%/hotfix/lib:%TRA_HOME%/lib:%TPCL_HOME%/lib:%JVM_LIB_SERVER_DIR%:%JVM_LIB_DIR%:%TRA_HOME%/hotfix/icjava/6.2/lib:%TRA_HOME%/icjava/6.2/lib:%LIBPATH%

#
# Specifies the Xalan compatibility issue to launch this application
#
#java.property.java.endorsed.dirs=%TPCL_HOME%/lib

#
# Specifies the JVM to be used to launch this application
#
java.library %JVM_LIB_PATH%

#
# Specifies the JVM to be used to launch this application
#
java.start.class com.tibco.pe.PEMain

#
# Specifies the main function in invoke
#
java.start.method main

#
# Specifies the javaCode directory to use
#
java.property.javaCode %BW_HOME%/lib/palettes

#
# Specifies the maximum Java heap size to allocate
#
java.heap.size.max %HEAP_SIZE%

#
# Specifies the TNT palettes directory
#
java.property.palettePath %BW_HOME%/lib/palettes:%BW_PLUGINS_HOME%/lib/palettes:%TRA_HOME%/lib/palettes

#
# Installation details
#
java.property.tibco_home %TIB_HOME%
java.property.tibco_tra_version %TRA_APPEND_VERSION%

#
# Specifies the classpath with expansion on
#
tibco.class.path.extended %CUSTOM_EXT_PREPEND_CP%:%STD_EXT_CP%:%CUSTOM_EXT_APPEND_CP%

#
# Sets the SAX and DOM JAXP factories to use
#
java.property.javax.xml.parsers.SAXParserFactory org.apache.xerces.jaxp.SAXParserFactoryImpl
java.property.javax.xml.parsers.DocumentBuilderFactory org.apache.xerces.jaxp.DocumentBuilderFactoryImpl

#
# Specifies the remaining arguments to pass into the application
#
application.args %APP_ARGS%
#
# Specify values to connect to the domain for authorization (http activity)
#
java.property.TIBCO_TRA_HOME %TRA_HOME%
java.property.TIBCO_DOMAIN_HOME %TRA_HOME%/../domain

#
# Specifies the Service Agent for Built-in Resource Provider
#
ServiceAgent.builtinResource.serviceagent.Class com.tibco.plugin.brp.BRPServiceAgent

# GOLD Settings
tibco.tibjms.connect.attempts = 10,10000
tibco.tibjms.connect.attempt.timeout = 30000

EnableMemorySavingMode = true
